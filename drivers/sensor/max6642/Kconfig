DT_COMPAT_MAX6642 := max6642

config MAX6642
	bool "MAX6642 Temperature Sensor"
	default $(dt_compat_enabled,$(DT_COMPAT_MAX6642))
	depends on I2C
	help
	  Enable the driver for the MAX6642 digital temperature Sensor with 2-wire
	  interface. Thermal watchdog is not implemented
